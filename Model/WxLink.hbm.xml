<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Model"
                   namespace="Model">
  <class name="Model.WxLink" table="WxLink">
    <id name="ID" type="int" column="ID">
      <generator class="native"/>
    </id>
    <property name="Title" column="Title" type="string" length="255"></property>
    <property name="Description" column="Description" type="StringClob"></property>
    <property name="Url" column="Url" type="string" length="500"></property>
    <property name="UpdateTime" column="UpdateTime" type="DateTime"></property>
  </class>
</hibernate-mapping>